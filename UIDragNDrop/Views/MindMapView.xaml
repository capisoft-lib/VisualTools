<?xml version="1.0" encoding="UTF-8"?>
<ContentView 
    xmlns="http://xamarin.com/schemas/2014/forms" 
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    x:Class="UIDragNDrop.Views.MindMapView">
    <ContentView.Content>
        <Frame IsClippedToBounds="true" Margin="5" Padding="5"
               CornerRadius="5" BorderColor="Black">
            <Label Text="{Binding Title, Mode=TwoWay}" />
        </Frame>
    </ContentView.Content>
</ContentView>
